{
  "definition": "./journal.rpc",
  "clients": {
    "typescript": {
      "output": "./out/service_client.gen.ts",
      "types": {
        "uuid": "string",
        "date": "string"
      }
    }
  },
  "servers": {
    "go-echo": {
      "output": "./out/server.go",
      "package": "main",
      "types": {
        "uuid": {
          "package": "github.com/google/uuid",
          "namespace": "uuid",
          "typeName": "UUID"
        },
        "date": {
          "package": "time",
          "namespace": "time",
          "typeName": "Time"
        }
      }
    }
  }
}
